						"exec": [
							"let getStatusCode = pm.response.code,\r",
							"statusCodeOk = 200,\r",
							"statusCodeBadRequest = 400,\r",
							"statusCodeUnauthorized = 401,\r",
							"folderParentName = pm.collectionVariables.get(\"setFolderName0\"),\r",
							"folderChildId = pm.collectionVariables.get(\"folderChildId\"),\r",
							"folderChildName = pm.collectionVariables.get(\"setFolderChildName0\");\r",
							"\r",
							"if (getStatusCode == statusCodeOk) {\r",
							"    pm.test(\"Получен успешный код ответа от сервера\", () => {\r",
							"        pm.test(\"Информация по найденным папкам успешно получена\", () => {\r",
							"            let responseJson = JSON.parse(responseBody),\r",
							"            count = responseJson.count;\r",
							"\r",
							"            if (count != 1) {\r",
							"                pm.test(\"Ошибка найденного количества записей в теле ответа\", () => {\r",
							"                    pm.expect(count).to.eql(1);\r",
							"                });\r",
							"                postman.setNextRequest(null);\r",
							"            } else {\r",
							"                for (var i = 0; i < count; i++) {\r",
